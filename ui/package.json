{
  "name": "clustercode",
  "private": true,
  "version": "master",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write ./src/**/*.{js,svelte,html,ts}",
    "lint": "eslint './src/**/*.{js,ts,svelte}'",
    "lint:fix": "eslint --fix './src/**/*.{js,ts,svelte}'",
    "prelint": "npm run format",
    "cy:open": "cypress open --component",
    "cy:run": "cypress run --component",
    "sass": "sass --load-path=node_modules src/routes/styles.scss src/routes/styles.css",
    "prebuild": "npm run sass",
    "test": "vitest"
  },
  "devDependencies": {
    "@cypress/svelte": "2.0.0",
    "@cypress/vite-dev-server": "5.0.5",
    "@sveltejs/adapter-static": "2.0.1",
    "@sveltejs/kit": "1.15.0",
    "@sveltejs/vite-plugin-svelte": "2.0.4",
    "@testing-library/cypress": "9.0.0",
    "@tsconfig/svelte": "4.0.1",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "carbon-components": "10.58.3",
    "carbon-components-svelte": "0.73.5",
    "carbon-icons-svelte": "11.4.0",
    "carbon-preprocess-svelte": "0.9.1",
    "cronstrue": "2.24.0",
    "cypress": "12.9.0",
    "dayjs": "1.11.7",
    "dotenv": "16.0.3",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-svelte3": "4.0.0",
    "jose": "4.13.1",
    "msw": "1.2.1",
    "prettier": "2.8.7",
    "prettier-plugin-svelte": "2.10.0",
    "sass": "1.60.0",
    "svelte": "3.58.0",
    "svelte-check": "3.1.4",
    "svelte-preprocess": "5.0.3",
    "tslib": "2.5.0",
    "typescript": "5.0.3",
    "vite": "4.2.1",
    "vitest": "0.29.8",
    "yaml": "2.2.1"
  }
}
